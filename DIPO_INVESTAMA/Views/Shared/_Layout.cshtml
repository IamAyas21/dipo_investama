<html style="height: auto; min-height: 100%;">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>DIPO INVESTAMA</title>
    <!-- Tell the browser to be responsive to screen width -->
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <!-- Bootstrap 3.3.7 -->
    <link href="~/Content/AdminLTE/css/bootstrap.min.css" rel="stylesheet" />
    <script src="~/Content/AdminLTE/js/chart/Chart.bundle.min.js"></script>
    @*<script src="~/Content/AdminLTE/js/chart/Chart.js"></script>*@
    <!-- Font Awesome -->
    <link href="~/Content/AdminLTE/fonts/font-awesome.min.css" rel="stylesheet" />
    <!-- Ionicons -->
    <link href="~/Content/AdminLTE/css/ionicons.min.css" rel="stylesheet" />

    @*<link href="~/Content/AdminLTE/css/morris.css" rel="stylesheet" />*@
    <!-- Theme style -->
    <link href="~/Content/AdminLTE/css/AdminLTE.min.css" rel="stylesheet" />
    <!-- AdminLTE Skins. Choose a skin from the css/skins
         folder instead of downloading all of them to reduce the load. -->
    <link href="~/Content/AdminLTE/css/_all-skins.min.css" rel="stylesheet" />
    <!-- jvectormap -->
    <link href="~/Content/AdminLTE/jvectormap/jquery-jvectormap.css" rel="stylesheet" />
    <!-- Date Picker -->
    <link href="~/Content/AdminLTE/datepicker/bootstrap-datepicker.min.css" rel="stylesheet" />
    <!-- Daterange picker -->
    <link href="~/Content/AdminLTE/datepicker/daterangepicker.css" rel="stylesheet" />
    <!-- bootstrap wysihtml5 - text editor -->
    <link href="~/Content/AdminLTE/plugins/bootstrap3-wysihtml5.min.css" rel="stylesheet" />
    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
    <!-- Google Font -->
    <link href="~/Content/myFont.css" rel="stylesheet" />
    <link href="~/Content/input.css" rel="stylesheet" />
    <link href="~/Content/layout.css" rel="stylesheet" />
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,600,700,300italic,400italic,600italic">
    <style type="text/css">
        .jqstooltip {
            position: absolute;
            left: 0px;
            top: 0px;
            visibility: hidden;
            background: rgb(0, 0, 0) transparent;
            background-color: rgba(0,0,0,0.6);
            filter: progid:DXImageTransform.Microsoft.gradient(startColorstr=#99000000, endColorstr=#99000000);
            -ms-filter: "progid:DXImageTransform.Microsoft.gradient(startColorstr=#99000000, endColorstr=#99000000)";
            color: white;
            font: 10px arial, san serif;
            text-align: left;
            white-space: nowrap;
            padding: 5px;
            border: 1px solid white;
            box-sizing: content-box;
            z-index: 10000;
        }

        .jqsfield {
            color: white;
            font: 10px arial, san serif;
            text-align: left;
        }
    </style>

    <!-- jQuery 3 -->
    <script async="" src="//www.google-analytics.com/analytics.js"></script>
    <script src="~/Content/AdminLTE/js/jquery.min.js"></script>
    <!-- jQuery UI 1.11.4 -->
    <script src="~/Content/AdminLTE/js/jquery-ui.min.js"></script>
    <!-- Resolve conflict in jQuery UI tooltip with Bootstrap tooltip -->
    <script>
        $.widget.bridge('uibutton', $.ui.button);
    </script>

    <!-- Bootstrap 3.3.7 -->
    <script src="~/Content/AdminLTE/js/bootstrap/bootstrap.min.js"></script>
    <!-- Morris.js charts -->
    @*<script src="~/Content/AdminLTE/js/raphael/raphael.min.js"></script>
        <script src="~/Content/AdminLTE/js/morris/morris.min.js"></script>*@
    <!-- Sparkline -->
    <script src="~/Content/AdminLTE/js/jquery-sparkline/jquery.sparkline.min.js"></script>
    <!-- jvectormap -->
    <script src="~/Content/AdminLTE/js/plugin/jvectormap/jquery-jvectormap-1.2.2.min.js"></script>
    <script src="~/Content/AdminLTE/js/plugin/jvectormap/jquery-jvectormap-world-mill-en.js"></script>
    <!-- jQuery Knob Chart -->
    <script src="~/Content/AdminLTE/js/plugin/jquery-knob/jquery.knob.min.js"></script>
    <!-- daterangepicker -->
    <script src="~/Content/AdminLTE/js/momen/moment.min.js"></script>
    <script src="~/Content/AdminLTE/js/bootstrap-daterangepicker/daterangepicker.js"></script>
    <!-- datepicker -->
    <script src="~/Content/AdminLTE/js/bootstrap-daterangepicker/bootstrap-datepicker.min.js"></script>
    <!-- Bootstrap WYSIHTML5 -->
    <script src="~/Content/AdminLTE/js/plugin/bootstrap3-wysihtml5.all.min.js"></script>
    <!-- Slimscroll -->
    <script src="~/Content/AdminLTE/js/jquery.slimscroll.min.js"></script>
    <!-- FastClick -->
    <script src="~/Content/AdminLTE/js/fastclick.js"></script>
    <!-- AdminLTE App -->
    <script src="~/Content/AdminLTE/js/adminlte.min.js"></script>
    <!-- AdminLTE dashboard demo (This is only for demo purposes) -->
    @*<script src="~/Content/AdminLTE/js/dashboard.js"></script>*@
    <!-- AdminLTE for demo purposes -->
    <script src="~/Content/AdminLTE/js/demo.js"></script>
    <script src="~/Scripts/Dropdownlist.js"></script>
    
    @RenderSection("scripts", required: false)
</head>
<body class="skin-blue sidebar-mini wysihtml5-supported" style="height: auto; min-height: 100%;">
    <div class="wrapper" style="height: auto; min-height: 100%;">

        <header class="main-header">
            <!-- Logo -->
            <a href="@Url.Action("Index","Home")" class="logo">
                <img src="~/Assets/logo_dipo_dashboard.png" style="margin-left: -8 !important; opacity:0.5;background-size:cover;background-repeat:space;background-position:center;" />
            </a>
            <!-- Header Navbar: style can be found in header.less -->
            <nav class="navbar navbar-static-top body-navbar-top">
                <!-- Sidebar toggle button-->
                <a href="#" class="sidebar-toggle" data-toggle="push-menu" role="button">
                    <span class="sr-only">Toggle navigation</span>
                </a>

                <div class="navbar-custom-menu">
                    <ul class="nav navbar-nav">

                        <!-- User Account: style can be found in dropdown.less -->
                        <li class="dropdown user user-menu">
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown" aria-expanded="false">
                                <img src="~/Assets/background_login_dipo.png" class="user-image" alt="User Image">
                                <span class="hidden-xs">@DIPO_INVESTAMA.Utils.SessionManager.name()</span>
                            </a>
                            <ul class="dropdown-menu">
                                <!-- User image -->
                                <li class="user-header" style="background-color:#001e6e">
                                    <img src="~/Assets/background_login_dipo.png" class="img-circle" alt="User Image">

                                    <p>
                                        @DIPO_INVESTAMA.Utils.SessionManager.name()
                                        @*<small>Member since Nov. 2012</small>*@
                                    </p>
                                </li>
                                <!-- Menu Body -->
                                <!-- Menu Footer-->
                                <li class="user-footer">
                                    @*<div class="pull-left">
                                            <a href="#" class="btn btn-default btn-flat">Profile</a>
                                        </div>*@
                                    <div class="pull-right">
                                        <a href="@Url.Action("Logout","Account")" class="btn btn-default btn-flat">Sign out</a>
                                    </div>
                                </li>
                            </ul>
                        </li>
                    </ul>
                </div>
            </nav>
        </header>
        <!-- Left side column. contains the logo and sidebar -->
        @Html.Action("Menu", "Menu", new { controller = ViewContext.RouteData.Values["Controller"].ToString(), action = ViewContext.RouteData.Values["Action"].ToString() })

        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper body-content-wrapper" style="min-height: 969.8px; height: auto !important;">
            <!-- Content Header (Page header) -->
            @*<section class="content-header">
                    <h1>
                        Dashboard
                        <small>Control panel</small>
                    </h1>
                    <ol class="breadcrumb">
                        <li><a href="#"><i class="fa fa-dashboard"></i> Home</a></li>
                        <li class="active">Dashboard</li>
                    </ol>
                </section>*@

            <!-- Main content -->
            <section class="content body-main" style="height: auto !important; min-height: 0px !important;">
                @RenderBody()
            </section>
            <!-- /.content -->
        </div>
        <!-- /.content-wrapper -->
        <footer class="main-footer">
            <div class="pull-right hidden-xs">
                <b>Version</b> 2.4.13
            </div>
            <strong>Copyright © 2014-2019 <a href="https://adminlte.io">AdminLTE</a>.</strong> All rights
            reserved.
        </footer>

        <!-- Control Sidebar -->
    
    </div>
    <!-- ./wrapper -->

</body>
</html>