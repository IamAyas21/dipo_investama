            <button type="button" class="close" data-dismiss="modal">×</button>
        </div>
        <div class="modal-body Poppins-Regular font-white">
            <div class="container">
                <div class="row">
                    @*<div class="col-md-4">
                        <label>Account No</label>
                        @Html.TextBoxFor(model => model.AccountNo, new { @class = "form-control", @placeholder = "Account No", autocomplete = "off", required = "required" })
                    </div>*@
                    <div class="col-md-4">
                        <label>Account Name</label>
                        @Html.TextBoxFor(model => model.AccountName, new { @class = "form-control", @placeholder = "Account Name", autocomplete = "off", required = "required" })
                    </div>
                </div>
            </div>
        </div>
        <div class="modal-footer">
            <button type="submit" class="btn btn-primary">Submit</button>
            <a type="button" class="btn btn-default" href="@Url.Action("Index","MasterAccount")">Kembali</a>
        </div>
    </div>
